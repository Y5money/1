_p.Network:get("PDS", "closePC", pcs.id, {
        ["m"] = {
            ["2"] = {
                [1] = 1, -- box
                [2] = 1 -- slot
            }
        },
        ["h"] = {
            ["2"] = true
        },
        ["cb"] = 1
    })
wait(.1)
_p.Network:get("PDS", "takeDCPokemon", 1)
wait(.1)
_p.Network:get("PDS", "takeDCPokemon", 1)
wait(.1)  
    end
}

